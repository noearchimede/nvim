{

  "vvv": {
    "prefix": "vvv",
    "description": "Volts",
    "body": "\\SI{$1\\}{\\volt\\}$0",
    "luasnip": {
      "autotrigger": true
    }
  },

  "template": {
    "prefix": "template",
    "description": "Basic template",
    "body": [
      "\\documentclass[12pt]{article\\}",
      "",
      "\\usepackage[utf8]{inputenc\\}",
      "\\usepackage[paper=a4paper]{geometry\\}",
      "\\usepackage{amsmath\\}",
      "\\usepackage{amssymb\\}",
      "\\usepackage[hidelinks]{hyperref\\}",
      "\\usepackage{graphicx\\}",
      "\\usepackage{subcaption\\}",
      "\\usepackage[font=small,labelfont=bf]{caption\\}",
      "\\usepackage[separate-uncertainty=true]{siunitx\\}",
      "",
      "\\usepackage{parskip\\}",
      "\\setlength{\\parindent\\}{0pt\\}",
      "",
      "\\renewcommand{\\phi\\}{\\varphi\\}",
      "\\renewcommand{\\theta\\}{\\vartheta\\}",
      "\\renewcommand{\\epsilon\\}{\\varepsilon\\}",
      "",
      "% Bibliography",
      "\\usepackage[style=numeric]{biblatex\\}",
      "\\addbibresource{bibliography.bib\\}",
      "",
      "\\title{\\}",
      "\\author{\\}",
      "\\date{\\}",
      "",
      "",
      "\\begin{document\\}",
      "",
      "\\maketitle",
      "",
      "\\begin{abstract\\}",
      "\\end{abstract\\}",
      "",
      "\\tableofcontents",
      "\\newpage",
      "",
      "",
      "",
      "\\end{document\\}"
    ]
  },

  "beg": {
    "prefix": "beg",
    "description": "begin{} / end{}",
    "body": [
      "\\begin{$1\\}",
      "\t$0",
      "\\end{$1\\}"
    ]
  },

  "fig": {
    "prefix": "fig",
    "description": "Figure environment",
    "body": [
      "\\begin{figure\\}${1:[]}",
      "\t\\centering",
      "\t\\includegraphics[width=\\textwidth]{$2\\}",
      "\t\\caption{$4\\}",
      "\t\\label{fig:$3\\}",
      "\\end{figure\\}"
    ]
  },

  "enum": {
    "prefix": "enum",
    "description": "Enumerate",
    "body": [
      "\\begin{enumerate\\}",
      "\t\\item $0",
      "\\end{enumerate\\}"
    ]
  },

  "item": {
    "prefix": "item",
    "description": "Itemize",
    "body": [
      "\\begin{itemize\\}",
      "\t\\item $0",
      "\\end{itemize\\}"
    ]
  },

  "pack": {

    "prefix": "pack",

    "description": "Package",

    "body": "\\usepackage[${1:options}]{${2:package}\\}$0"

  },

  "=>": {
    "prefix": "=>",
    "description": "implies",
    "body": "\\implies"
  },

  "=<": {
    "prefix": "=<",
    "description": "implied by",
    "body": "\\impliedby"
  },

  "dm": {
    "prefix": "dm",
    "description": "Math",
    "body": [
      "\\[",
      "${1:$TM_SELECTED_TEXT}",
      "\\] $0"
    ]
  },

  "equation": {
    "prefix": "equation",
    "description": "Equation",
    "body": [
      "\\begin{equation\\}",
      "\t$2",
      "\t\\label{eq:$1\\}",
      "\\end{equation\\}"
    ]
  },

  "align": {
    "prefix": "align",
    "description": "Align",
    "body": [
      "\\begin{align\\}",
      "\t$2",
      "\t\\label{eq:$1\\}",
      "\\end{align\\}"
    ]
  },

  "*equation": {
    "prefix": "*equation",
    "description": "Equation",
    "body": [
      "\\begin{equation*\\}",
      "\t$1",
      "\\end{equation*\\}"
    ]
  },

  "*align": {
    "prefix": "*align",
    "description": "Align",
    "body": [
      "\\begin{align*\\}",
      "\t$1",
      "\\end{align*\\}"
    ]
  },

  "//": {
    "prefix": "//",
    "description": "Simple fraction",
    "body": "\\frac{$TM_SELECTED_TEXT\\}{$1\\}$0"
  },

  "frac": {
    "prefix": "frac",
    "description": "Fraction",
    "body": "\\frac{$1\\}{$2\\}$0"
  },

  "sympy": {
    "prefix": "sympy",
    "description": "sympyblock ",
    "body": "sympy $1 sympy$0"
  },

  "pmat": {
    "prefix": "pmat",
    "description": "pmat",
    "body": "\\begin{pmatrix\\} $1 \\end{pmatrix\\} $0"
  },

  "bmat": {
    "prefix": "bmat",
    "description": "bmat",
    "body": "\\begin{bmatrix\\} $1 \\end{bmatrix\\} $0"
  },

  "sum": {
    "prefix": "sum",
    "description": "sum",
    "body": "\\sum_{n=${1:1}\\}^{${2:\\infty}\\} ${3:a_n z^n}"
  },

  "lim": {
    "prefix": "lim",
    "description": "limit",
    "body": "\\lim_{${1:n} \\to ${2:\\infty}\\} "
  },

  "limsup": {
    "prefix": "limsup",
    "description": "limsup",
    "body": "\\limsup_{${1:n} \\to ${2:\\infty}\\} "
  },

  "part": {
    "prefix": "part",
    "description": "d/dx",
    "body": "\\frac{\\partial $1\\}{\\partial ${2:x}\\} $0"
  },

  "sqrt": {
    "prefix": "sqrt",
    "description": "\\sqrt{}",
    "body": "\\sqrt{${1:$TM_SELECTED_TEXT}\\} $0"
  },

  "__": {
    "prefix": "__",
    "description": "subscript",
    "body": "_{$1\\}$0"
  },

  "EE": {
    "prefix": "EE",
    "description": "exists",
    "body": "\\exists "
  },

  "AA": {
    "prefix": "AA",
    "description": "forall",
    "body": "\\forall "
  },

  "xx": {
    "prefix": "xx",
    "description": "cross",
    "body": "\\times "
  },

  "**": {
    "prefix": "**",
    "description": "cdot",
    "body": "\\cdot ",
    "luasnip": {
      "priority": 100
    }
  },

  "norm": {
    "prefix": "norm",
    "description": "norm",
    "body": "\\|$1\\|$0"
  },

  "int": {
    "prefix": "int",
    "description": "integral",
    "body": "\\int_{${1:-\\infty}\\}^{${2:\\infty}\\} ${3:$TM_SELECTED_TEXT} $0",
    "luasnip": {
      "priority": 300
    }
  },

  "<->": {
    "prefix": "<->",
    "description": "leftrightarrow",
    "body": "\\leftrightarrow",
    "luasnip": {
      "priority": 200
    }
  },

  "set": {
    "prefix": "set",
    "description": "set",
    "body": "\\{$1\\\\} $0"
  },

  "tt": {
    "prefix": "tt",
    "description": "text",
    "body": "\\text{$1\\}$0"
  },

  "case": {
    "prefix": "case",
    "description": "cases",
    "body": [
      "\\begin{cases\\}",
      "\t$1",
      "\\end{cases\\}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },

  "bigfun": {
    "prefix": "bigfun",
    "description": "Big function",
    "body": [
      "\\begin{align*\\}",
      "\t$1: $2 &\\longrightarrow $3 \\\\",
      "\t$4 &\\longmapsto $1($4) = $0",
      ".\\end{align*\\}"
    ]
  },

  "cvec": {
    "prefix": "cvec",
    "description": "column vector",
    "body": "\\begin{pmatrix\\} ${1:x}_${2:1}\\\\ \\vdots\\\\ $1_${2:n} \\end{pmatrix\\}"
  }

}
